{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Search",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "minLength": 1
      },
      "name": {
        "type": "string",
        "minLength": 1
      },
      "image": {
        "type": "string"
      },
      "infoUrl": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "name"
    ]
  },
  "minItems": 1
}